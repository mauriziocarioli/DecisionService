package com.toyota.tfs.decisionservice;

import java.lang.Number;

rule "Set Early Stage"
	dialect "mvel"
	when
		a : AccountHolder( daysDelinquent >= 12 && daysDelinquent <= 45 )
	then
		modify( a ) {
				setCurrentStage( "Early" )
		}
		insert( new EarlyStageIsSet() );
		System.out.println("===> FQHC-Pricing: firing rule "+drools.getRule().getName());
end
