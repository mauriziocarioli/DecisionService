package com.toyota.tfs.decisionservice;

rule "Late Stage 2"
	dialect "mvel"
	when
		a : AccountHolder( currentStage == "Late" , rflag != "R" , BALANCE >= 1500 , LIST_1 > "1587" , LIST_1 < "1750" )
		h : Helper( )
	then
		modify( a ) {
				setLIST_1( h.getListID("1550","1587","1750","1795") )
		}
end
