package com.toyota.tfs.decisionservice;

import java.lang.Number;

rule "Set Mid Stage"
	dialect "mvel"
	when
		a : AccountHolder( daysDelinquent >= 46 && daysDelinquent <= 89 )
	then
		System.out.println("===> DecisionService: firing rule "+drools.getRule().getName()+", ID="+a.getID());
		modify( a ) {
				setCurrentStage( "Mid" ), 
				setTreatmentAudit( drools.getRule().getName() )
		}
		insert( new MidStageIsSet() );
end
